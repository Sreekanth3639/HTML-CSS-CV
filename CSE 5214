        "import pandas as pd\n",
        "import numpy as np\n",
        "\n",
        "# Step 1: Define dataset with student scores\n",
        "student_scores = pd.DataFrame(\n",
        "    np.random.randint(50, 100, size=(4, 3)),\n",
        "    index=['S1', 'S3', 'S5', 'S7'],\n",
        "    columns=['Math', 'Science', 'English']\n",
        ")\n",
        "\n",
        "# Step 2: Reindex to add more students (introducing NaN for missing data)\n",
        "all_students = ['S1', 'S2', 'S3', 'S4', 'S5', 'S6', 'S7', 'S8']\n",
        "student_scores = student_scores.reindex(all_students)\n",
        "\n",
        "# Step 3: Print results\n",
        "print(\"Dataset with Missing Values (Cleansing 1):\")\n",
        "print(student_scores)\n"
